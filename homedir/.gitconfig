[alias]
    st = status
    ci = commit
    co = checkout
    br = branch
[user]
	name = Bill Mill
	email = "bill.mill@gmail.com"
[color]
    branch = auto
    diff = auto
    status = auto

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = cyan

[push]
    default = upstream
[credential]
	helper = osxkeychain
[diff]
	algorithm = patience
